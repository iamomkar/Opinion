<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activities.VoteSuccessActivity">

    <ImageView
        android:layout_width="150dp"
        android:id="@+id/success_logo"
        android:layout_height="150dp"
        app:srcCompat="@drawable/ic_verified_user_teal_700_24dp"
        android:layout_centerInParent="true"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/success_logo"
        android:layout_marginTop="15dp"
        android:textStyle="bold"
        android:textSize="20sp"
        android:text="Vote Successfully Registered"
        android:layout_centerHorizontal="true"
        />

</RelativeLayout>